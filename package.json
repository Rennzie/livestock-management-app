{
  "name": "stockman-api",
  "version": "0.0.1",
  "main": "index.js",
  "author": "sean rennie",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "src/*"
    ]
  },
  "scripts": {
    "start:server": "nodemon --exec babel-node src/index.js",
    "start:client": "cd client && yarn start:client",
    "clean": "rm -rf dist && mkdir dist",
    "seed": "babel-node src/db/seeds",
    "build": "yarn clean && babel src --preset minify --out-dir dist",
    "heroku-postbuild": "yarn build && cd client && yarn && yarn build",
    "start": "node dist/index.js",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "debug": "node ./testRef.js",
    "test": "nyc mocha --require @babel/register test/**/*_spec.js --require test/spec_helper --recursive --exit"
  },
  "engines": {
    "node": "9.10.0",
    "yarn": "1.12.3"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "express": "^4.16.4",
    "express-static-gzip": "^1.1.3",
    "fast-csv": "^2.4.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mongoose": "^5.3.14",
    "mongoose-unique-validator": "^2.0.1",
    "morgan": "^1.9.0",
    "multer": "^1.4.1",
    "supertest": "^3.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-minify": "^0.5.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.8.3",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3"
  }
}
